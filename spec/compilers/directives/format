component Main {
  fun render : String {
    try {
      {result, formatted} =
        @format {
          "HelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHelloH" \
          "Bello"
        }

      result + formatted
    }
  }
}
--------------------------------------------------------------------------------
class A extends _C {
  render() {
    return (() => {
      const [a,b] = [`HelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHBello`, `"HelloHelloHelloHelloHelloHelloHelloHelloHelloHelloHelloH" \\
"Bello"`];

      return a + b;
    })();
  }
};

A.displayName = "Main";
